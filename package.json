    "vue-blu": "^0.1.9",
    "vue-chartkick": "^0.2.0",
    "vue-electron": "^1.0.6",
    "vue-google-autocomplete": "^1.0.13",
    "vue-moment": "^2.0.2",
    "vue-resource": "^1.3.4",
    "vue-router": "^2.5.3",
    "vue-shortkey": "^2.2.1",
    "vuedraggable": "^2.14.1",
    "vuex": "^2.3.1"
  },
